{
    "version": 2,
    "builds": [
      {
        "src": "api/users.ts",
        "use": "@vercel/node"
      },
      {
        "src": "api/flights.ts",
        "use": "@vercel/node"
      },
      {
        "src": "api/bookings.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users(.*)",
        "dest": "/api/users.ts"
      },
      {
        "src": "/api/flights(.*)",
        "dest": "/api/flights.ts"
      },
      {
        "src": "/api/bookings(.*)",
        "dest": "/api/bookings.ts" 
      }
    ]
  }